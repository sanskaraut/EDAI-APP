<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5DAAE3"
    android:backgroundTint="#82B2DB">

    <TextView
        android:id="@+id/textView53"
        android:layout_width="320dp"
        android:layout_height="148dp"
        android:background="#E18CE7F3"
        android:gravity="center"
        android:text="Car washing at door step"
        android:textSize="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.152" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="320dp"
        android:layout_height="148dp"
        android:layout_marginTop="15dp"
        android:background="#E18CE7F3"
        android:gravity="center"
        android:text="Pick-up and return washing service"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView53"
        app:layout_constraintVertical_bias="0.064" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="320dp"
        android:layout_height="148dp"
        android:layout_marginTop="15dp"
        android:background="#E18CE7F3"
        android:gravity="center"
        android:text="Car washing appointment"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView54"
        app:layout_constraintVertical_bias="0.108" />
</androidx.constraintlayout.widget.ConstraintLayout>