        android:drawableRight="@drawable/baseline_arrow_forward_ios_24"
        android:gravity="center|left"
        android:paddingLeft="12dp"
        android:text="Make Appointment"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35"
        app:layout_constraintVertical_bias="0.074" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="390dp"
        android:layout_height="90dp"
        android:layout_marginTop="25dp"
        android:background="#5DAAE3"
        android:drawableRight="@drawable/baseline_arrow_forward_ios_24"
        android:gravity="center|left"
        android:paddingLeft="12dp"
        android:text="View Appointment History"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView51"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="415dp"
        android:layout_height="83dp"
        android:background="#FF5DAAE3"
        android:backgroundTint="#FF5DAAE3"
        android:gravity="center"
        android:paddingLeft="30dp"
        android:text="AutoHub"
        android:textColor="#090909"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
